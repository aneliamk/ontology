<#tt0083929>
	rdf:type schema:Movie;
	mcb:hasId "tt0083929";
	mcb:hasTitle "Fast Times at Ridgemont High (1982)  IMDb";
	schema:duration "1h30min";
	schema:dateCreated "1982";
