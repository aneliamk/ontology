<#tt0412689-erma12>
	schema:about <#tt0412689>;
	schema:author "erma12";
	schema:dateCreated "Thu Jun 29 2006 00:00:00 GMT+0200 (Mitteleurop√§ische Sommerzeit)";
	schema:headline " A Great Movie!";
