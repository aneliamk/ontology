<#tt0033105-robertc39>
	schema:about <#tt0033105>;
	schema:author "robertc39";
	schema:dateCreated "Sat Dec 20 2003 00:00:00 GMT+0100 (Mitteleurop√§ische Normalzeit)";
	schema:reviewRating 5;
	schema:headline " great";
