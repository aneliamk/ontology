	schema:author "disdressed12";
	schema:dateCreated "Sun Jul 13 2008 00:00:00 GMT+0200 (Mitteleuropäische Sommerzeit)";
	schema:reviewRating 5;
	schema:headline " a B movie with underwritten story and characters";
