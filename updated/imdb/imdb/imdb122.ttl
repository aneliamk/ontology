<#tt0097179-Phroggy>
	schema:about <#tt0097179>;
	schema:author "Phroggy";
	schema:dateCreated "Thu Jul 22 1999 00:00:00 GMT+0200 (Mitteleuropäische Sommerzeit)";
	schema:reviewRating 7;
	schema:headline " A good timewaster";
