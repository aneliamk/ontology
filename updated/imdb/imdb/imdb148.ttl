<#tt0412888-abrakhim>
	schema:about <#tt0412888>;
	schema:author "abrakhim";
	schema:dateCreated "Tue Sep 06 2005 00:00:00 GMT+0200 (Mitteleuropäische Sommerzeit)";
	schema:reviewRating 8;
	schema:headline " something special ...";
